ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/SwiftyVISPER" "$PODS_CONFIGURATION_BUILD_DIR/VISPER-Redux" "$PODS_CONFIGURATION_BUILD_DIR/VISPER-Wireframe"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/SwiftyVISPER/SwiftyVISPER.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/VISPER-Redux/VISPER_Redux.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/VISPER-Wireframe/VISPER_Wireframe.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "SwiftyVISPER" -framework "VISPER_Redux" -framework "VISPER_Wireframe"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
